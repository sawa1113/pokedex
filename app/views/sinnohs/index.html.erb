<p id="notice"><%= notice %></p>

<h1>Sinnohs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type 1</th>
      <th>Type 2</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sinnohs.each do |sinnoh| %>
      <tr>
        <td><%= sinnoh.name %></td>
        <td><%= sinnoh.type_1 %></td>
        <td><%= sinnoh.type_2 %></td>
        <td><%= sinnoh.height %></td>
        <td><%= sinnoh.weight %></td>
        <td><%= sinnoh.description %></td>
        <td><%= link_to 'Show', sinnoh %></td>
        <td><%= link_to 'Edit', edit_sinnoh_path(sinnoh) %></td>
        <td><%= link_to 'Destroy', sinnoh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sinnoh', new_sinnoh_path %>
