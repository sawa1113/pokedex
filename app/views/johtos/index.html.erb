<p id="notice"><%= notice %></p>

<h1>Johtos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type 1</th>
      <th>Type 2</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @johtos.each do |johto| %>
      <tr>
        <td><%= johto.name %></td>
        <td><%= johto.type_1 %></td>
        <td><%= johto.type_2 %></td>
        <td><%= johto.height %></td>
        <td><%= johto.weight %></td>
        <td><%= johto.description %></td>
        <td><%= link_to 'Show', johto %></td>
        <td><%= link_to 'Edit', edit_johto_path(johto) %></td>
        <td><%= link_to 'Destroy', johto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Johto', new_johto_path %>
